class Solution {
    public int[] corpFlightBookings(int[][] bookings, int n) {
       int len = bookings.length;
       int[] ans = new int[n];
       for(int i = 0 ; i< len ; i++){
           int[] booking = bookings[i];
           int f = booking[0]-1;
           int t = booking[1]-1;
           int seat = booking[2];
           for(int j = f ; j<=t; j++){
               ans[j]+=seat;
           }
       } 
        return ans;
    }
}
